{
  "name": "heimdall-monitor",
  "version": "1.0.0",
  "description": "Heimdal Monitor - Internet connection monitoring server with web dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start server.js --name heimdall-monitor",
    "pm2:stop": "pm2 stop heimdall-monitor",
    "pm2:restart": "pm2 restart heimdall-monitor",
    "pm2:delete": "pm2 delete heimdall-monitor",
    "install-global": "npm install -g pm2 nodemon"
  },
  "keywords": [
    "internet",
    "monitoring",
    "connectivity",
    "speedtest",
    "dashboard",
    "norse",
    "yggdrasil"
  ],
  "author": "luxumbra",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luxumbra/internet-monitor.git"
  }
}
